<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>


<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualigue.gui.AddObstacleController">
   <children>
      <HBox>
         <children>
            <VBox spacing="20.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label text="Name:" VBox.vgrow="ALWAYS">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets left="5.0" top="3.0" />
                     </VBox.margin>
                  </Label>
                  <VBox>
                     <children>
                        <Label text="Picture:" />
                        <ImageView fx:id="preview" fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <VBox spacing="30.0" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="name" />
                  <Button mnemonicParsing="false" onAction="#importPicture" text="Browse..." />
               </children>
            </VBox>
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#cancel" text="Cancel" />
            <Button mnemonicParsing="false" onAction="#confirm" text="Confirm" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
   </children>
</VBox>
