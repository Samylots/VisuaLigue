<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<ToolBar orientation="VERTICAL" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <items>
      <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="50.0" text="Add player">
         <graphic>
            <StackPane prefHeight="43.0" prefWidth="49.0">
               <children>
                  <ImageView fitHeight="37.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/player.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="25.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true" translateX="6.0" translateY="10.0">
                     <image>
                        <Image url="@icons/add.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
         </graphic>
         <padding>
            <Insets top="1.0" />
         </padding>
      </Button>
      <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="50.0" text="Add obstacle">
         <graphic>
            <StackPane prefHeight="43.0" prefWidth="49.0">
               <children>
                  <ImageView fitHeight="37.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/obstacle.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="25.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true" translateX="6.0" translateY="10.0">
                     <image>
                        <Image url="@icons/add.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
         </graphic>
         <padding>
            <Insets top="1.0" />
         </padding>
      </Button>
      <Button mnemonicParsing="false" prefHeight="45.0" prefWidth="50.0" text="Add obstacle">
         <graphic>
            <StackPane prefHeight="43.0" prefWidth="49.0">
               <children>
                  <ImageView fitHeight="37.0" fitWidth="41.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@icons/accessory.png" />
                     </image>
                  </ImageView>
                  <ImageView fitHeight="25.0" fitWidth="18.0" pickOnBounds="true" preserveRatio="true" translateX="6.0" translateY="10.0">
                     <image>
                        <Image url="@icons/add.png" />
                     </image>
                  </ImageView>
               </children>
            </StackPane>
         </graphic>
         <padding>
            <Insets top="1.0" />
         </padding>
      </Button>
   </items>
</ToolBar>
