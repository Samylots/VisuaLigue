<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" collapsible="false" text="Game creation mode 1 manipulation board" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualigue.gui.javafx.fxcontrollers.states.FrameByFrameBoardController">
   <content>
      <VBox>
         <children>
            <HBox spacing="10.0">
               <children>
                  <Label text="Actual frame:" />
                  <Label fx:id="frameLabel" text="0" />
               </children>
            </HBox>
            <Slider fx:id="frameSlider" blockIncrement="1.0" majorTickUnit="5.0" max="0.0" minorTickCount="1" />
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#deleteFrame" text="Button">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@icons/delete.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#previous" text="&lt;&lt;" />
                  <Button mnemonicParsing="false" onAction="#next" text="&gt;&gt;" />
                  <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#addFrame" text="Button">
                     <graphic>
                        <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@icons/add.png" />
                           </image>
                        </ImageView>
                     </graphic>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
