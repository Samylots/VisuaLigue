<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" collapsible="false" text="Game creation mode 2 manipulation board" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualigue.gui.javafx.fxcontrollers.states.RealTimeBoardController">
   <content>
      <VBox>
         <children>
            <HBox spacing="10.0">
               <children>
                  <Label text="Actual time:" />
                  <Label fx:id="frameLabel" text="0" />
               </children>
            </HBox>
            <Slider fx:id="frameSlider" blockIncrement="1.0" majorTickUnit="10.0" max="0.0" minorTickCount="5" showTickLabels="true" showTickMarks="true" />
            <HBox alignment="CENTER" spacing="30.0">
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
