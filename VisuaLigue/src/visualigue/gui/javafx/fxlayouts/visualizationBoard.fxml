<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" collapsible="false" text="Game time  manipulation board" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualigue.gui.javafx.fxcontrollers.states.VisualizationBoardController">
   <content>
      <VBox>
         <children>
            <Slider fx:id="timeSlider" />
            <HBox alignment="CENTER" spacing="30.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#backwrdStep" text="|&lt;&lt;" />
                  <Button mnemonicParsing="false" onAction="#backward" text="&lt;&lt;" />
                  <Button mnemonicParsing="false" onAction="#play" text="Play" />
                  <Button mnemonicParsing="false" onAction="#pause" text="Pause" />
                  <Button mnemonicParsing="false" onAction="#foward" text="&gt;&gt;" />
                  <Button mnemonicParsing="false" onAction="#fowardStep" text="&gt;&gt;|" />
                  <TextField fx:id="timeConfig" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
