<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TitledPane animated="false" collapsible="false" text="Game time  manipulation board" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="visualigue.gui.javafx.fxcontrollers.states.VisualizationBoardController">
   <content>
      <VBox>
         <children>
            <Slider fx:id="timeSlider" />
            <HBox alignment="CENTER" spacing="15.0">
               <children>
                  <HBox alignment="CENTER" spacing="10.0">
                     <children>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#backwardStep" text="|&lt;&lt;">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <rotationAxis>
                                    <Point3D />
                                 </rotationAxis>
                                 <viewport>
                                    <Rectangle2D height="60.0" minY="200.0" width="60.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#backward" text="&lt;&lt;">
                           <graphic>
                              <ImageView fitHeight="40.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="60.0" minY="100.0" width="110.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#play" text="Play">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="60.0" minX="200.0" width="60.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#pause" text="Pause">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="55.0" minX="105.0" width="50.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#stop" text="Stop">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="60.0" minX="-2.0" minY="-1.0" width="60.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#foward" text="&gt;&gt;">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="60.0" minX="150.0" minY="100.0" width="110.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                        <Button contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#fowardStep" text="&gt;&gt;|">
                           <graphic>
                              <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@icons/buttons.png" />
                                 </image>
                                 <viewport>
                                    <Rectangle2D height="70.0" minX="190.0" minY="195.0" width="70.0" />
                                 </viewport>
                              </ImageView>
                           </graphic>
                        </Button>
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" spacing="5.0">
                     <children>
                        <TextField fx:id="timeConfig" maxWidth="80.0" />
                        <Label text="frames" />
                     </children>
                  </HBox>
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </content>
</TitledPane>
